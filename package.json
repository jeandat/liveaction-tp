{
    "name": "liveaction-tp",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "prestart": "npm run lint && npm run check:conf",
        "start": "ng serve --verbose --aot --host 0.0.0.0 --proxy-config src/proxy.conf.js",
        "start:prod": "npm run configure -- --prod && npm run start -- --prod",
        "prebuild": "npm run lint && npm run check:conf",
        "build": "ng build --aot",
        "build:prod": "npm run configure -- --prod && npm run build -- --prod",
        "build:stats": "npm run configure -- --prod --no-mocks && npm run build -- --prod --stats-json --source-map",
        "stats:se": "cross-var source-map-explorer $npm_config_bundle $npm_config_bundle.map",
        "stats:wba": "cross-var webpack-bundle-analyzer dist/$npm_package_name/stats.json",
        "stats:bb": "cross-var bundle-buddy dist/$npm_package_name/*.js.map",
        "stats": "npm run build:stats && npm run stats:wba",
        "lint": "ng lint --format stylish",
        "pretest": "npm run check:conf",
        "test": "ng test --browsers=Chrome",
        "test:ci": "ng test --code-coverage --watch=false --progress=false --browsers=MyHeadlessChrome",
        "e2e": "ng e2e --port 4201",
        "prerelease": "npm run check:ci",
        "release": "cross-var npm version $npm_config_new_version -m \"chore: Mark version v%s\"",
        "postrelease": "git push && git push --tags",
        "configure": "node src/conf/instrument.js",
        "check:conf": "node src/conf/check-conf.js",
        "check:ci": "npm ci && npm run build:prod && npm run test:ci && npm run e2e",
        "compodoc": "compodoc -p src/tsconfig.app.json -d public/compodoc -n 'Live Action'",
        "pregitbook": "which gitbook | grep -s gitbook || (npm i -g gitbook-cli && gitbook fetch && gitbook install)",
        "gitbook": "gitbook build . public",
        "gen:docs": "npm run gitbook && npm run compodoc",
        "gen:svg-sprite": "gulp svg",
        "serve:docs": "npm run gen:docs && cd public && http-server"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^7.1.1",
        "@angular/cdk": "^7.1.0",
        "@angular/common": "~7.1.1",
        "@angular/compiler": "~7.1.1",
        "@angular/core": "~7.1.1",
        "@angular/forms": "~7.1.1",
        "@angular/http": "~7.1.1",
        "@angular/material": "^7.1.0",
        "@angular/platform-browser": "~7.1.1",
        "@angular/platform-browser-dynamic": "~7.1.1",
        "@angular/router": "~7.1.1",
        "@ngrx/effects": "^6.1.2",
        "@ngrx/entity": "^6.1.2",
        "@ngrx/router-store": "^6.1.2",
        "@ngrx/store": "^6.1.2",
        "@ngrx/store-devtools": "^6.1.2",
        "angular-in-memory-web-api": "^0.7.0",
        "core-js": "^2.5.4",
        "hamburgers": "^1.1.3",
        "hammerjs": "^2.0.8",
        "highcharts": "^7.0.1",
        "highcharts-angular": "^2.3.1",
        "human-format": "^0.10.1",
        "ngx-loading": "^3.0.1",
        "rxjs": "~6.3.3",
        "zone.js": "~0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.10.0",
        "@angular/cli": "~7.0.6",
        "@angular/compiler-cli": "~7.1.1",
        "@angular/language-service": "~7.1.1",
        "@compodoc/compodoc": "^1.1.5",
        "@ngrx/schematics": "^6.1.2",
        "@types/jasmine": "~2.8.8",
        "@types/jasmine-expect": "^3.8.1",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "bundle-buddy": "^0.2.1",
        "codelyzer": "~4.5.0",
        "cross-var": "^1.1.0",
        "gulp": "^3.9.1",
        "gulp-rename": "^1.4.0",
        "gulp-svgmin": "^2.1.0",
        "gulp-svgstore": "^7.0.0",
        "http-server": "^0.11.1",
        "include-media": "^1.4.9",
        "jasmine-core": "~2.99.1",
        "jasmine-expect": "^4.0.0",
        "jasmine-marbles": "^0.4.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~3.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~1.1.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "karma-jasmine-matchers": "^3.8.3",
        "ngrx-store-freeze": "^0.2.4",
        "protractor": "~5.4.0",
        "puppeteer": "^1.10.0",
        "replace": "^1.0.0",
        "rxjs-tslint-rules": "^4.14.0",
        "shelljs": "^0.8.2",
        "source-map-explorer": "^1.6.0",
        "ts-node": "~7.0.0",
        "tslint": "~5.11.0",
        "typescript": "~3.1.6",
        "webpack-bundle-analyzer": "^3.0.3",
        "yargs-parser": "^11.0.0"
    }
}
